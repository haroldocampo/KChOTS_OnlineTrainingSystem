<div id="createform" class="hform">
    <form id="eventForm">
    <h3 class="center">Create Event</h3>
    <table class="center">
        <tr>
    	    <td>Title</td>
       	    <td><input type="text" name="title"/></td>
            <td colspan = "2"></td>
        </tr>
        <tr>
    	    <td>Start Date</td>
       	    <td><input id="startDate" type="text" name="start"/></td>
            <td colspan = "2"></td>
        </tr>
        <tr>
    	    <td>End Date</td>
       	    <td><input id="endDate" type="text" name="end"/></td>
            <td colspan = "2"></td>
        </tr>
    </table>
    <span><input type="button" id="submitEvent" style="float:none;" class="submit" value="Create" /></span>
    <span><input type="button" id="close" style="float:none;" class="submit" value="Close" /></span>
    </form>
</div>